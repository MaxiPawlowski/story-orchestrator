{
  "name": "extension-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "cross-env NODE_ENV=production webpack --mode production --watch",
    "dev": "cross-env NODE_ENV=development concurrently \"webpack --mode development --watch\" \"tsc --noEmit --watch\"",
    "typecheck": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.24.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^13.0.1",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint-config-react-app": "^7.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.4.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  }
}
